<ivy-module version="2.0"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation="https://ant.apache.org/ivy/schemas/ivy.xsd">
  <info organisation="us.fatehi.schemacrawler" module="velocity"/>
  <configurations>
    <conf name="binaries" visibility="public" />
  </configurations>
  <dependencies>
    <dependency org="org.apache.velocity"
                name="velocity-engine-scripting" rev="2.4.1" conf="binaries->default" />
    <dependency org="org.apache.velocity"
                name="velocity-engine-core" rev="2.4.1" conf="binaries->default" />

    <exclude org="org.slf4j" module="slf4j-simple" />
    <exclude org="org.slf4j" module="slf4j-api" />
    <exclude org="junit" module="junit" />
    <exclude org="org.hamcrest" module="hamcrest-core" />
  </dependencies>
</ivy-module>
